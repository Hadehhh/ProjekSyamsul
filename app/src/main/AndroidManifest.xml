<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.projek_syamsul" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.UJKSyamsul"
        tools:targetApi="31" >
        <activity
            android:name="SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.projek_syamsul.dapur.DapurActivity"
            android:exported="false" />
        <activity
            android:name="com.example.projek_syamsul.pesanan.ListPesananActivity"
            android:exported="false" />
        <activity
            android:name="com.example.projek_syamsul.pesanan.PesananActivity"
            android:exported="false" />
        <activity
            android:name="com.example.projek_syamsul.menu.DetailListKategoriActivity"
            android:exported="false" />
        <activity
            android:name="com.example.projek_syamsul.menu.ListKategoriActivity"
            android:exported="false" />
        <activity
            android:name="com.example.projek_syamsul.menu.KategoriActivity"
            android:exported="false" />
        <activity
            android:name="com.example.projek_syamsul.DashboardActivity"
            android:exported="false" >
        </activity>
    </application>

</manifest>